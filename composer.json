{
  "name": "oliverde8/page-compose",
  "type": "library",
  "description": "A library allowing to make the composition of pages configurable.",
  "license": "MIT",
  "authors": [
    {
      "name": "Oliver de Cramer",
      "homepage": "http://oliver-decramer.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "oliverde8/associative-array-simplified" : "1.0.*",
    "symfony/property-access" : "3.4.*|3.3.*",
    "psr/simple-cache" : "*",
    "psr/cache" : "*",
    "guzzlehttp/promises": "1.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "6.5.*",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/cache": "3.4.*|3.3.*"
  },
  "autoload": {
    "psr-4": {
      "Oliverde8\\PageCompose\\": "src/Oliverde8/PageCompose"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Oliverde8\\PageCompose\\Tests\\": "tests/Oliverde8/PageCompose/Tests"
    }
  }
}
